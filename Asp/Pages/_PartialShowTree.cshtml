@using NodesDLL
@model INode
@if (Model.nodes.Any())
{
    @foreach (var node in Model.nodes)
     {
    <div class="partial">
        <div class="partial-text-id">@node.Unit.name</div>
        <div class="partial-text-id">@node.Unit.position</div>
        <div class="partial-text-id">@node.Unit.fullname</div>
        <a class="btn btn-info btn-additional-style"
           asp-page="./DetailTree"
           asp-route-nodeId="@node.nodeId">
            <i class="btn-text">Detail</i>
        </a>
        @await Html.PartialAsync(@"~/Pages/_PartialShowTree.cshtml", node)
    </div>
     }
}